{
  "name": "csv-to-javascript",
  "version": "1.0.0",
  "main": "index.js",
  "author": "ryanspice",
  "license": "none",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "description": "visualize csv",
  "scripts": {
    "build": "cd ./packages/angular && yarn && ng build",
    "start": "cd ./packages/angular && yarn && ng serve",
    "csv-to-javascript": "node packages/csv-to-javascript",
    "angular": "cd ./packages/angular && yarn && ng",
    "angular:install": "cd ./packages/angular && yarn",
    "angular:serve": "cd ./packages/angular && yarn && ng serve --open",
    "angular:build": "cd ./packages/angular && yarn && ng build",
    "angular:test": "cd ./packages/angular && yarn && ng test",
    "react": "cd ./packages/react && npm start",
    "react:install": "cd ./packages/react && npm i",
    "react:serve": "cd ./packages/react && npm start --open",
    "react:build": "cd ./packages/react && npm run-script build",
    "react:test": "cd ./packages/react && npm run-script test",
    "vanilla": "http-server ./packages/vanilla -o"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "commander": "^4.0.1",
    "csv-loader": "^3.0.2",
    "http-server": "^0.12.0",
    "papaparse": "^5.1.0",
    "path": "^0.12.7"
  },
  "bin": "./index.js"
}
